@model MyCV.Models.EducationListModel
<form action="/Home/AddEducation" method="post" role="form">
    <table class="table table-striped">
        @foreach (var education in Model.EducationList)
        {
            <tr>

                <td>@education.Begin-@education.End</td>
                <td>@education.SchoolName</td>
                <td><a href="/Home/DeleteEducation?Id=@education.Id" class="btn btn-danger">X</a></td>
            </tr>
        }
            <tr>
                <td>
                    <input type="text" name="Begin" class="form-control" placeholder="Год начала"> -
                    <input type="text" name="End" class="form-control" placeholder="Год окончания">
                </td>
                <td><input type="text" name="SchoolName" class="form-control" placeholder="Имя"></td>
                <td><input type="submit" class="btn btn-success" value="Add" /></td>
            </tr>
    </table>
</form>

