@model MyCV.Models.WorkExperienceListViewModel

@foreach (var workExperience in Model.WorkExperienceList)
{
<dl class="row">
    <dt class="col-sm-3">Даты</dt>
    <dd class="col-sm-9">@workExperience.Begin-@workExperience.End</dd>

    <dt class="col-sm-3">Место работы</dt>
    <dd class="col-sm-9">@workExperience.WorkName</dd>

    <dt class="col-sm-3">Должность</dt>
    <dd class="col-sm-9">@workExperience.PositionName</dd>

    <dt class="col-sm-3">Навыки</dt>
    <dd class="col-sm-9">@string.Join(", ", workExperience.Skills.Select(x => x.Name))</dd>
</dl>
}
